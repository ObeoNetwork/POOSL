    get-property "getItems().TreeItem[0].getItems().TreeItem[0].getItems().TreeItem[8].getText()" | equals destination4 
        | verify-true
	
    get-property "getItems().TreeItem[0].getItems().TreeItem[0].getItemCount()" | equals 9 | verify-true
