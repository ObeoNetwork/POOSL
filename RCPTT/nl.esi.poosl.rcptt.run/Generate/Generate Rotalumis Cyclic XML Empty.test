get-view "Project Explorer" | get-tree | select "CyclicProject/models/FileA.poosl" | get-menu "Generate Legacy Rotalumis XML" 
    | click


with [get-view "Project Explorer" | get-tree] {
    select "CyclicProject/models/FileA.poosl" | get-menu "Generate Legacy Rotalumis XML" | click
    select "CyclicProject/models/simulator/FileA.xml" | double-click
}
get-editor "FileA.xml" | get-tree | get-item "poosl_specification" | assert-non-empty
get-editor "FileA.xml" | get-tree | get-item "poosl_specification" | get-property childCount | equals 5 | verify-true

get-editor "FileA.xml" | get-tree | select "poosl_specification/process_classes/process_class/name"
with [get-editor "FileA.xml" | get-tree] {
    get-item "poosl_specification/process_classes/process_class/name" | get-property "values['Node']" | equals name 
        | verify-true
    get-item "poosl_specification/process_classes/process_class/name" | get-property "values['Content']" 
        | equals processClass | verify-true
}
