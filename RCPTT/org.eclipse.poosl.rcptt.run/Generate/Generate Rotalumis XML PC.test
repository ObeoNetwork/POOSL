Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree 
    | select "org.eclipse.poosl.examples/models-basic/ProducerConsumer/producer_consumer.poosl" 
    | get-menu "Generate Legacy Rotalumis XML" | click
get-view "Project Explorer" | get-tree | get-item "org.eclipse.poosl.examples/models-basic/ProducerConsumer" 
    | get-property childCount | equals 2 | verify-true
with [get-view "Project Explorer" | get-tree] {
    get-item "org.eclipse.poosl.examples/models-basic/ProducerConsumer/simulator" | assert-non-empty
    get-item "org.eclipse.poosl.examples/models-basic/ProducerConsumer/simulator" | get-property childCount | equals 1 
        | verify-true
}
get-view "Project Explorer" | get-tree 
    | select "org.eclipse.poosl.examples/models-basic/ProducerConsumer/simulator/producer_consumer.xml" | double-click

with [get-editor "producer_consumer.xml" | get-tree] {
    get-property "getItems().TreeItem[0].getText()" | equals xml | verify-true
    get-property "getItems().TreeItem[1].getText()" | equals "poosl_specification" | verify-true
    get-property "getItems().TreeItem[1].getItemCount()" | equals 5 | verify-true
    get-property "getItemCount()" | equals 2 | verify-true
}
