with [get-view "Project Explorer" | get-tree] {
	select "nl.esi.poosl.examples/models-basic/HelloWorld/hello_world.poosl" | get-menu "Run As/ *. POOSL Simulation" | click
}

try -times 10 -delay 50 -command {
	with [get-view Console] {
		// HINT: label output is slightly since eclipse 2020-06
		//get-label "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | assert-non-empty
		get-label | get-text | contains "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | verify-true
	}
} -finally {
	with [get-view Console] {
		get-button Terminate | get-property enablement | equals false | verify-true
		// HINT: label output is slightly since eclipse 2020-06
		//get-label "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | assert-non-empty
		get-label | get-text | contains "<terminated> hello_world.poosl [POOSL Simulation] [Seed: 1]" | verify-true
	}
	get-text-viewer | get-property text | contains terminated | verify-true
}
