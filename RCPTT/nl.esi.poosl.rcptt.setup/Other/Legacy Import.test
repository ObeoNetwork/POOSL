Content-Type: text/ecl
Entry-Name: .content

let [val workspace [get-workspace-location]] [val file "\CleanProject\models\legacy.xml"] 
{
	get-menu "File/Import..." | click
	get-window Import | get-tree | select "POOSL/Import Legacy SHESim XML as Poosl" | double-click
	get-window "File Import Wizard" | get-editbox -after [get-label "Select File:"] | set-text [format "%s%s" [str $workspace] [str $file]]
}
with [get-window "File Import Wizard"] {
    get-tree | select "CleanProject/models"
    get-button Finish | click
}
with [get-view "Project Explorer" | get-tree] {
    get-item "CleanProject/models" | get-property childCount | equals 3 | verify-true
    get-item "CleanProject/models/legacy.poosl" | assert-non-empty
    select "CleanProject/models/legacy.poosl" | double-click
}

get-editor "legacy.poosl" | get-text-viewer | get-property text | equals "" | verify-false
