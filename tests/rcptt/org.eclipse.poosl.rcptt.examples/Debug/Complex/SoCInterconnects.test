		get-button Terminate 
		| get-property enablement 
		| equals false 
		| verify-true
		// HINT: label output is slightly since eclipse 2020-06
		//get-label "<terminated> mesh.poosl [POOSL Simulation] [Seed: 1]" | assert-non-empty
