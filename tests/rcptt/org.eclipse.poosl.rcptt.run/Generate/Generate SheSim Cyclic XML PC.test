Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | select "CyclicProject/models/FileA.poosl" 
    | get-menu "Generate Legacy SHESim XML" | click

with [get-view "Project Explorer" | get-tree] {
    get-item "CyclicProject/models/simulator" | assert-non-empty
    get-item "CyclicProject/models/simulator/FileA.xml" 
        | assert-non-empty
}
get-view "Project Explorer" | get-tree 
    | select "CyclicProject/models/simulator/FileA_shesim.xml" | double-click
get-editor "FileA_shesim.xml" | get-tab-folder | get-tab-item Design | click
with [get-editor "FileA_shesim.xml" | get-tree] {
  get-item "poosl_specification" | assert-non-empty
  get-item "poosl_specification" | get-property childCount | equals 5 | verify-true
    
}
get-editor "FileA_shesim.xml" | get-tree | select "poosl_specification/process_classes/process_class/name"
with [get-editor "FileA_shesim.xml" | get-tree] {
    get-item "poosl_specification/process_classes/process_class/name" | get-property "values['Node']" | equals name 
        | verify-true
    get-item "poosl_specification/process_classes/process_class/name" | get-property "values['Content']" 
        | equals processClass | verify-true
}
